{
  "detectRetina": true,
  "fpsLimit": 60,
  "autoPlay": true,
  "backgroundMask": {
    "composite": "destination-out",
    "cover": {
      "color": {
        "value": "#fff"
      },
      "opacity": 1
    },
    "enable": false
  },
  "defaultThemes": {},
  "delay": 0,
  "fullScreen": {
    "enable": false,
    "zIndex": 1
  },
  "interactivity": {
    "detectsOn": "canvas",
    "events": {
      "onHover": {
        "enable": true,
        "mode": "bubble"
      },
      "resize": true
    },
    "modes": {
      "bubble": {
        "distance": 20,
        "duration": 2,
        "opacity": 8,
        "size": 10,
        "speed": 3
      }
    }
  },
  "particles": {
    "color": {
      "value": "#DC143C",
      "animation": {
        "enable": false,
        "speed": 0.15,
        "sync": true
      }
    },
    "lineLinked": {
      "blink": false,
      "color": "#DC143C",
      "consent": false,
      "distance": 18,
      "enable": true,
      "opacity": 3,
      "width": 3
    },
    "move": {
      "attract": {
        "enable": false,
        "rotate": {
          "x": 600,
          "y": 1200
        }
      },
      "bounce": false,
      "direction": "none",
      "enable": true,
      "outMode": "bounce",
      "random": false,
      "speed": 0.1,
      "straight": true
    },
    "number": {
      "density": {
        "enable": false,
        "area": 3000
      },
      "limit": 1500,
      "value": 1500
    },
    "opacity": 1,
    "shape": {
      "type": "triangle"
    },
    "size": {
      "animation": {
        "enable": false,
        "minimumValue": 1,
        "speed": 20,
        "sync": false
      },
      "random": true,
      "value": 1
    }
  },
  "polygon": {
    "draw": {
      "enable": true,
      "lineColor": "rgba(255,255,255,0.2)",
      "lineWidth": 0.5
    },
    "move": {
      "radius": 10
    },
    "inlineArrangement": "per-point",
    "scale": 2,
    "type": "inline",
    "url": "/2eul.svg"
  }
  }
