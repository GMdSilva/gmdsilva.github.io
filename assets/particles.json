{
  "detectRetina": false,
  "fpsLimit": 60,
  "autoPlay": true,
  "backgroundMask": {
    "composite": "destination-out",
    "cover": {
      "color": {
        "value": "#fff"
      },
      "opacity": 0.01
    },
    "enable": false
  },
  "defaultThemes": {},
  "delay": 0,
  "fullScreen": {
    "enable": false,
    "zIndex": 1
  },
  "interactivity": {
    "detectsOn": "canvas",
    "events": {
      "onHover": {
        "enable": true,
        "mode": "bubble"
      },
      "resize": true
    },
    "modes": {
      "bubble": {
        "distance": 20,
        "duration": 2,
        "opacity": 10,
        "size": 10,
        "speed": 3
      }
    }
  },
  "particles": {
    "color": {
      "value": "#aaff00",
      "animation": {
        "enable": true,
        "speed": 20,
        "sync": true
      }
    },
    "lineLinked": {
      "blink": false,
      "color": "random",
      "consent": false,
      "distance": 45,
      "enable": true,
      "opacity": 0.25,
      "width": 1
    },
    "move": {
      "attract": {
        "enable": false,
        "rotate": {
          "x": 600,
          "y": 1200
        }
      },
      "bounce": false,
      "direction": "none",
      "enable": true,
      "outMode": "bounce",
      "random": true,
      "speed": 0.35,
      "straight": false
    },
    "number": {
      "density": {
        "enable": false,
        "area": 3000
      },
      "limit": 750,
      "value": 300
    },
    "opacity": 0.2,
    "shape": {
      "type": "circle"
    },
    "size": {
      "animation": {
        "enable": false,
        "minimumValue": 1,
        "speed": 20,
        "sync": false
      },
      "random": true,
      "value": 0.01
    }
  },
  "polygon": {
    "draw": {
      "enable": true,
      "lineColor": "rgba(255,255,255,0.2)",
      "lineWidth": 0.1
    },
    "move": {
      "radius": 30
    },
    "inlineArrangement": "equidistant",
    "scale": 2.4,
    "type": "inline",
    "url": "1v0d.svg"
  }
  }
